<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuPrincipalActivity"
    android:background="#484848"

    >

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_marginTop="30dp"
    >

    <com.google.android.material.button.MaterialButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:width="150dp"
        android:text="Voltar"

        android:layout_marginBottom="5dp"
        style="@style/Widget.Material3.Button.OutlinedButton"
        app:icon="@drawable/baseline_arrow_back"
        margin android:layout_marginRight="20dp"
        android:layout_marginLeft="20dp"
        android:textColor="#cccc"
        app:cornerRadius="2dp"
        android:textColorHint="#2A2929"
        app:strokeWidth="2dp"
        app:strokeColor="#2A2929"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        />
    <ImageView

        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="60dp"
        android:src="@drawable/logomouse"
        android:scaleType="centerCrop"
        />


</LinearLayout>

</LinearLayout>